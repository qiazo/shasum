{
  "name": "ts-shasum",
  "version": "1.2.0",
  "description": "Compute and check SHA256 message digest.",
  "keywords": [
    "sha256sum",
    "shasum"
  ],
  "homepage": "https://github.com/kjxbyz/shasum",
  "bugs": "https://github.com/kjxbyz/shasum/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjxbyz/shasum.git"
  },
  "license": "MIT",
  "author": "kjxbyz <kjxbyz888@outlook.com> (https://github.com/kjxbyz)",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.mts",
        "default": "./dist/main.mjs"
      },
      "require": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    },
    "./version": {
      "import": {
        "types": "./dist/version.d.mts",
        "default": "./dist/version.mjs"
      },
      "require": {
        "types": "./dist/version.d.ts",
        "default": "./dist/version.js"
      }
    }
  },
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "bin": {
    "shasum": "bin/shasum.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "glob": "^11.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@types/node": "18.19.83",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.0",
    "sort-package-json": "^2.14.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "glob": "^11.0.0"
  },
  "packageManager": "pnpm@10.2.1",
  "publishConfig": {
    "access": "public"
  }
}
