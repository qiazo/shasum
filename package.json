{
  "name": "@insco/shasum",
  "version": "1.3.1",
  "description": "Compute and check SHA256 message digest.",
  "keywords": [
    "sha256sum",
    "shasum"
  ],
  "homepage": "https://github.com/insco-inc/shasum",
  "bugs": "https://github.com/insco-inc/shasum/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insco-inc/shasum.git"
  },
  "license": "MIT",
  "author": "kjxbyz <hello@kjxbyz.com> (https://github.com/kjxbyz)",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.mts",
        "default": "./dist/main.mjs"
      },
      "require": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    },
    "./version": {
      "import": {
        "types": "./dist/version.d.mts",
        "default": "./dist/version.mjs"
      },
      "require": {
        "types": "./dist/version.d.ts",
        "default": "./dist/version.js"
      }
    }
  },
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "bin": {
    "shasum": "bin/shasum.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chalk": "^5.4.1",
    "consola": "^3.4.2",
    "glob": "^11.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@types/node": "20.19.13",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "sort-package-json": "^3.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "glob": "^11.0.0"
  },
  "packageManager": "pnpm@10.2.1",
  "publishConfig": {
    "access": "public"
  }
}
