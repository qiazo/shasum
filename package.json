{
  "name": "@insco/shasum",
  "version": "1.3.2",
  "description": "Compute and check SHA256 message digest.",
  "keywords": [
    "sha256sum",
    "shasum"
  ],
  "homepage": "https://github.com/qiazo/shasum",
  "bugs": "https://github.com/qiazo/shasum/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiazo/shasum.git"
  },
  "license": "MIT",
  "author": "kjxbyz <hello@kjxbyz.com> (https://github.com/kjxbyz)",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.mts",
        "default": "./dist/main.mjs"
      },
      "require": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    },
    "./version": {
      "import": {
        "types": "./dist/version.d.mts",
        "default": "./dist/version.mjs"
      },
      "require": {
        "types": "./dist/version.d.ts",
        "default": "./dist/version.js"
      }
    }
  },
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "bin": {
    "shasum": "bin/shasum.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chalk": "^5.6.2",
    "consola": "^3.4.2",
    "glob": "^11.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/node": "20.19.30",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "sort-package-json": "^3.6.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "glob": "^11.0.0"
  },
  "packageManager": "pnpm@10.2.1"
}
